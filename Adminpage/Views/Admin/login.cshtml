@*  *@
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <title>Đăng nhập</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="icon" type="image/png" href="~/Assets/res/images/icons/favicon.ico" />

    <link rel="stylesheet" type="text/css" href="~/Assets/res/vendor/bootstrap/css/bootstrap.min.css">

    <link rel="stylesheet" type="text/css" href="~/Assets/res/fonts/font-awesome-4.7.0/css/font-awesome.min.css">

    <link rel="stylesheet" type="text/css" href="~/Assets/res/vendor/animate/animate.css">

    <link rel="stylesheet" type="text/css" href="~/Assets/res/vendor/css-hamburgers/hamburgers.min.css">

    <link rel="stylesheet" type="text/css" href="~/Assets/res/vendor/select2/select2.min.css">

    <link rel="stylesheet" type="text/css" href="~/Assets/res/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/Assets/res/css/main.css">
   
</head>
<body ng-app="myApp" ng-controller="loginAdmin">
    <div class="limiter">
        <div class="container-login100">
            <div class="wrap-login100">
                <div class="login100-pic js-tilt" data-tilt>
                    <img src="~/Assets/res/images/img-01.png" alt="IMG">
                </div>
                <form class="login100-form validate-form">
                    <span class="login100-form-title">
                        LOGIN
                    </span>
                    <div class="wrap-input100 validate-input" data-validate="Valid email is required: ex@abc.xyz">
                        <input ng-model="nv.email" class="input100" type="text" name="email" placeholder="Email">
                        <span class="focus-input100"></span>
                        <span class="symbol-input100">
                            <i class="fa fa-envelope" aria-hidden="true"></i>
                        </span>
                    </div>
                    <div class="wrap-input100 validate-input" data-validate="Password is required">
                        <input ng-model="nv.matkhau" class="input100" type="password" name="pass" placeholder="Password">
                        <span class="focus-input100"></span>
                        <span class="symbol-input100">
                            <i class="fa fa-lock" aria-hidden="true"></i>
                        </span>
                    </div>
                    <div class="container-login100-form-btn">
                        <button ng-click="login()" class="login100-form-btn">
                            Login
                        </button>
                    </div>
                    <div class="text-center p-t-12">
                        <span class="txt1">
                            Forgot
                        </span>
                        <a class="txt2" href="#">
                            Username / Password?
                        </a>
                        <br>
                    </div>

                </form>
            </div>
        </div>
    </div>

    <script src="~/Assets/res/vendor/jquery/jquery-3.2.1.min.js"></script>

    <script src="~/Assets/res/vendor/bootstrap/js/popper.js"></script>
    <script src="~/Assets/res/vendor/bootstrap/js/bootstrap.min.js"></script>

    <script src="~/Assets/res/vendor/select2/select2.min.js"></script>

    <script src="~/Assets/res/vendor/tilt/tilt.jquery.min.js"></script>
    <script>
        $('.js-tilt').tilt({
            scale: 1.1
        })
    </script>

    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'UA-23581568-13');
    </script>

    <script src="~/Assets/res/js/main.js"></script>
    <toaster-container toaster-options="{'time-out': 3000, 'close-button':true, 'position-class':'toast-top-full-width'}"></toaster-container>
    <script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>
    <link href="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css" rel="stylesheet">
    <script src="~/Assets/js/Angular/angular.min.js"></script>
    <script src="~/Assets/js/Angular/angular-imgur-upload.js"></script>
    <script src="~/Assets/js/Angular/Srcripts/AngularAdmin.js"></script>
</body>

